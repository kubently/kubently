{
  "name": "kubently-cli",
  "version": "2.1.2",
  "description": "Modern Node.js CLI for Kubently - Troubleshooting Kubernetes Agentically",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "kubently": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "kubently": "node run.js",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write 'src/**/*.ts'",
    "update-local": "./update-local.sh",
    "rebuild": "npm run clean && npm run build",
    "watch": "tsc --watch",
    "dev:debug": "npm run build && node dist/index.js --api-url http://localhost:8080 --api-key test-api-key debug",
    "link:refresh": "npm unlink -g kubently-cli 2>/dev/null || true && npm link && asdf reshim nodejs"
  },
  "keywords": [
    "kubernetes",
    "k8s",
    "debugging",
    "cli",
    "kubently",
    "a2a"
  ],
  "author": "Kubently Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.12.2",
    "chalk": "^5.3.0",
    "commander": "^14.0.2",
    "figlet": "^1.9.3",
    "inquirer": "^12.10.0",
    "js-yaml": "^4.1.0",
    "open": "^10.1.0",
    "ora": "^9.0.0",
    "prompts": "^2.4.2",
    "readline-sync": "^1.4.10",
    "uuid": "^13.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/figlet": "^1.7.0",
    "@types/inquirer": "^9.0.9",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.9.1",
    "@types/prompts": "^2.4.9",
    "@types/readline-sync": "^1.4.8",
    "@types/uuid": "^11.0.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.38.0",
    "express": "^5.1.0",
    "jest": "^30.2.0",
    "prettier": "^3.4.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
